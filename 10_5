package come.a;


public class aa {
    public static void main(String[] args) {
       /* int r = GetMax(10, 20);
        System.out.println(r);*/
        System.out.println(GetMax(10,20));

    }

    public static int GetMax(int a, int b) {
        if (a > b) {
            return a;
        } else {
            return b;
        }

    }
}


